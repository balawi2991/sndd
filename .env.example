# ============================================
NODE_ENV=production
PORT=3000
JWT_EXPIRES_IN=7d
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
FRONTEND_URL=https://your-app.railway.app
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=30
